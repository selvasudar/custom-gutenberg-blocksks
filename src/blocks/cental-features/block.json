{
  "$schema": "https://schemas.wp.org/wp/trunk/block.json",
  "apiVersion": 3,
  "name": "cgb/features-section",
  "version": "1.0.0",
  "title": "Features Section",
  "category": "design",
  "icon": "star-filled",
  "description": "A feature highlight section with icons, image and text.",
  "keywords": ["features", "icons", "highlight"],
  "supports": {
    "html": false,
    "align": ["full", "wide"]
  },
  "attributes": {
    "heading": {
      "type": "string",
      "source": "html",
      "selector": "h1",
      "default": "Cental <span class='text-primary'>Features</span>"
    },
    "description": {
      "type": "string",
      "source": "html",
      "selector": "p",
      "default": "Lorem ipsum dolor sit amet, consectetur adipisicing elit..."
    },
    "imageUrl": {
      "type": "string",
      "default": "https://placehold.co/400x400"
    }
  },
  "textdomain": "custom-gutenberg-blocks"
}